{
  "name": "A-Tractor",
  "readme": "src/docs/README.md",
  "includeVersion": true,
  "entryPoints": ["src/common", "src/widgets", "src/utils", "src/_deprecated"],
  "entryPointStrategy": "expand",
  "out": "docs/public",
  "disableSources": true,
  "projectDocuments": ["src/docs/pages/**/*.md"],
  "exclude": [
    "**/node_modules/**",
    "**/*.test.ts",
    "**/*.spec.ts"
  ],
  "plugin": ["typedoc-material-theme"],
  "themeColor": "#fff",
  "skipErrorChecking": true,
  "excludeExternals": true,
  "excludePrivate": true,
  "excludeProtected": true,
  "categorizeByGroup": true,
  "searchInComments": true,
  "githubPages": false,
  "hideGenerator": true,
  "cleanOutputDir": true,
  "theme": "default"
}
